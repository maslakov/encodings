<?xml version="1.0"?>
<configuration>
    <configSections>
        <!--logger can be extracted in separate project-->
        <section name="logger" type="Encodings.Log.LoggerSection, Encodings" 
                 allowDefinition="Everywhere" 
                 allowExeDefinition="MachineToApplication" 
                 restartOnExternalChanges="true"/>
    </configSections>
    
	<appSettings>
		<!--<add key="logFileName" value="log.txt"/> -->
		<add key="tableFileName" value="App_Data\encodings.txt"/>
	</appSettings>
    <logger>
        <LogOutput fileName="log.txt"/>
    </logger>
    
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
    </startup>


</configuration>
